{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node index.js",
    "startdev": "nodemon index.js"
  },
  "dependencies": {
    "@splidejs/splide": "Glidias/splide",
    "@splidejs/vue-splide": "Glidias/vue-splide",
    "@vue/cli-plugin-babel": "4.1.1",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "chordsheetjs": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "marked": "^2.0.0",
    "node-libcurl": "^2.3.0",
    "socket.io": "^3.0.3",
    "socket.io-client": "^3.1.1",
    "v-namespace": "1.0.1",
    "vue": "^2.6.11",
    "vue-socket.io": "^3.0.10",
    "vue2-touch-events": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-service": "4.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "nodemon": "^2.0.7",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "starter"
  ],
  "description": "Vue.js example starter project"
}
